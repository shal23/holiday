<%= simple_form_for(@travel_agent_profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :company_name %>
    <%= f.input :address %>
    <%= f.input :company_url %>
    <%= f.input :about %>
    <%= f.input :avatar, :as => :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
